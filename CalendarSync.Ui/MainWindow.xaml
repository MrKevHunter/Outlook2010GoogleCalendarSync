<Window x:Class="CalendarSync.Ui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:syncSettings="clr-namespace:CalendarSync.Ui.Controls"
        Title="Outlook Sync Settings" Height="350" Width="525">
    <Border Margin="10,10,10,10" BorderBrush="Red" BorderThickness="1,1,1,1" CornerRadius="8,8,8,8">
        <Border.Background>
            <LinearGradientBrush EndPoint="0.504,1.5" StartPoint="0.504,0.03">
                <GradientStop Color="#FF25DCF9" Offset="0"/>
                <GradientStop Color="#FFFFFFFF" Offset="0.567"/>
            </LinearGradientBrush>
        </Border.Background>
        <syncSettings:SyncSettings >
            
        </syncSettings:SyncSettings>
    </Border>
</Window>
